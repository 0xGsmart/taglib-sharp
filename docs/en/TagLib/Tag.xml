    <Member MemberName="Artists">
      <MemberSignature Language="C#" Value="public virtual string[] Artists { set; get; };" />
      <MemberType>Property</MemberType>
      <ReturnValue>
        <ReturnType>System.String[]</ReturnType>
      </ReturnValue>
      <Docs>
        <summary>Gets and sets the list of artists who created the song.</summary>
        <value>A <see cref="T:System.String" />[] containing a list of the artists associated with the song.</value>
        <remarks>This field should contain band names such as "The Who", "Cake", or "Black Sabbath". Performer names should be stored in <see cref="P:TagLib.Tag.Performers" />.</remarks>
      </Docs>
    </Member>
